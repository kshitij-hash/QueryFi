# WalletConnect - Get from https://cloud.walletconnect.com
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=

# Alchemy - Get from https://dashboard.alchemy.com
NEXT_PUBLIC_ALCHEMY_API_KEY=

# Yellow Network - Agent address that receives micropayments in app sessions
NEXT_PUBLIC_AGENT_ADDRESS=

# OpenRouter - Get from https://openrouter.ai/keys
OPENROUTER_API_KEY=

# Circle - Treasury wallet (receives settled USDC via Uniswap v4 hook)
# Get from https://console.circle.com
CIRCLE_API_KEY=
CIRCLE_ENTITY_SECRET=
CIRCLE_WALLET_ID=
CIRCLE_TREASURY_ADDRESS=

# Settlement - Hook addresses (Base Sepolia default is hardcoded, override here if redeployed)
SETTLEMENT_HOOK_ADDRESS=

# Arc testnet - Hook address for multi-chain settlement (optional)
# Deploy hook to Arc testnet and set this to enable Arc settlement
ARC_SETTLEMENT_HOOK_ADDRESS=

# Neon Postgres - Connection string for settlement state persistence
# Get from https://console.neon.tech
DATABASE_URL=

# Agent private key for settlement calls and wallet balance checks (testnet only)
AGENT_PRIVATE_KEY=
